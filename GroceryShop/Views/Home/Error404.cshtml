@{
    ViewBag.Title = "Error 404-Not Found!";

}

<section class="flat-row flat-error">
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <div class="wrap-error text-center">
                    <div class="header-error">
                        404
                    </div>
                    <div class="content-error">
                        <h2>Oops, Page Not Found!</h2>
                        <p>It looks like nothing was found at this location. Click <a href="@Url.Action("Index", "Home")" class="btn-error">here</a> to return Homepage</p>
                    </div>
                </div><!-- /.wrap-error -->
            </div><!-- /.col-md-12 -->
        </div><!-- /.row -->
    </div><!-- /.container -->
</section><!-- /.flat-row -->
